{
	"name": "smuuf/celery-for-php",
	"description": "A modern Celery client for modern PHP.",
	"license": "MIT",
	"type": "library",
	"authors": [
		{
			"name": "Premysl Karbula"
		}
	],
	"support": {
		"issues": "https://github.com/smuuf/celery-for-php/issues",
		"source": "https://github.com/smuuf/celery-for-php"
	},
	"autoload":{
		 "psr-4": {
			 "Smuuf\\CeleryForPhp\\": "src/"
		 }
	},
	"require": {
		"php": ">=8.0",
		"symfony/polyfill-php81": "^1.26"
	},
	"require-dev": {
		"nette/tester": "^2.5",
		"phpstan/phpstan": "^2.1",
		"predis/predis": "^2",
		"php-amqplib/php-amqplib": "^3.6",
		"friendsofphp/php-cs-fixer": "^3.37"
	},
	"suggest": {
		"predis/predis": "Adds support for Predis - a PHP library providing a Redis result backend"
	}
}
